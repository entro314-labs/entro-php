{
    "name": "entro314labs/entro-php",
    "version": "1.0.0",
    "description": "PHP SDK for Entrolytics - First-party growth analytics for the edge",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "entrolytics",
        "analytics",
        "tracking",
        "web-analytics",
        "laravel"
    ],
    "homepage": "https://entrolytics.click",
    "authors": [
        {
            "name": "Entro314 Labs",
            "email": "hey@entrolytics.click"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laravel/framework": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Entrolytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Entrolytics\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Entrolytics\\Laravel\\EntrolyticsServiceProvider"
            ],
            "aliases": {
                "Entrolytics": "Entrolytics\\Laravel\\EntrolyticsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}